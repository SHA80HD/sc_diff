<?xml version="1.0" encoding="utf-8"?>
<ATLConfig xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" atl_name="SC_MXGS_Global_Music">
	<AudioTriggers>
		<ATLTrigger atl_name="Play_Global_Music" path="Events/Music/Global_Music_Soundtrack/Global_Music_Soundtrack">
			<WwiseEvent wwise_duration_type="Infinite" wwise_name="Play_Global_Music" />
			<RTPCDependees>
				<ATLRTPCDependee atl_name="Conversation_Effect" />
				<ATLRTPCDependee atl_name="UI_Effect" />
				<ATLRTPCDependee atl_name="SC_Music_Intensity" />
				<ATLRTPCDependee atl_name="SC_Music_Player_Mood" />
				<ATLRTPCDependee atl_name="SC_Music_Player_Tension" />
				<ATLRTPCDependee atl_name="Ship_Health" />
				<ATLRTPCDependee atl_name="propagation_pressurePhysicalAudible" />
				<ATLRTPCDependee atl_name="Control_Mood" />
				<ATLRTPCDependee atl_name="SC_Music_StarMarine_Control" />
				<ATLRTPCDependee atl_name="SC_Music_StarMarine_Timelimit" />
				<ATLRTPCDependee atl_name="SC_Music_StarMarine_Phase_Timelimit" />
				<ATLRTPCDependee atl_name="Player_FOIP_Input_State" />
				<ATLRTPCDependee atl_name="MIX_DR" />
				<ATLRTPCDependee atl_name="DM_Music_Diegetic_Vs_Cinematic" />
				<ATLRTPCDependee atl_name="MIX_Mono" />
			</RTPCDependees>
			<ATLPreload atl_name="SC_MXGS_Global_Music" />
		</ATLTrigger>
		<ATLTrigger atl_name="Stop_Global_Music" path="Events/Music/Global_Music_Soundtrack/Global_Music_Soundtrack">
			<WwiseEvent wwise_duration_type="OneShot" wwise_name="Stop_Global_Music" wwise_radius_max="0" />
			<RTPCDependees />
			<ATLPreload atl_name="SC_MXGS_Global_Music" />
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="SC_MXGS_Global_Music" atl_type="AutoLoad" path="Banks/">
			<ATLPlatforms>
				<Platform atl_config_group_name="default" atl_name="pc" />
			</ATLPlatforms>
			<ATLConfigGroup atl_name="default">
				<WwiseFile wwise_name="MXGS_Global_Music.bnk" />
			</ATLConfigGroup>
		</ATLPreloadRequest>
	</AudioPreloads>
	<AudioExternalSources />
	<AudioEnvironments />
	<AudioRTPCs />
	<AudioSwitches />
</ATLConfig>